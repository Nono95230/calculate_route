{{ attach_library('calculate_route/google-map') }}
<div id="container_maps">
	
	<div id="container_options">
		<div id="adress-start">
			<label id="label_start" class="label_style">{{ data.labelStart }}</label>
			<input id="start" name="start" placeholder="{{ data.phStart }}" autocomplete="off" type="text">
		</div>
		<div id="switch"><span><i class="fa fa-exchange fa-2x fa-rotate-90" aria-hidden="true"></i></span></div>
		<div id="adress-end">
			<label id="label_end" class="label_style">{{ data.labelEnd }}</label>
			<select id="end" name="end">
				<option value="{{ data.address }} ">{{ data.addressTitle }} </option>
			</select>
		</div>
		<div id="choice_mode">
			<span id="car" class="active" data-choice="car"></span>
			<span id="public_transport" data-choice="public_transport"></span>
			<span id="bike" data-choice="bike"></span>
			<span id="walker" data-choice="walker"></span>
		</div>
		<span id="cr_submit">{{ data.textSubmit }}</span>
	</div>

	<div id="map"></div>

	<div id="warnings_panel"></div>

	<div class="print">
		<span>Imprimer</span>
		<div class="sub_print">
			<p class="print1">Itin√©raire</p>
		</div>
	</div>

	<div id="directions-panel"></div>

	<div id="panel"></div>
</div>

